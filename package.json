{
  "name": "gloam-http",
  "version": "1.0.2",
  "author": "Moxy",
  "description": "基于java-gloam开发框架封装的http基础能力包",
  "type": "module",
  "files": [
    "dist",
    "types",
    "lib"
  ],
  "main": "./dist/gloamHttp.umd.cjs",
  "module": "./dist/gloamHttp.js",
  "types": "./types/index.d.ts",
  "exports": {
    "types": "./types/index.d.ts",
    "import": "./dist/gloamHttp.js",
    "require": "./dist/gloamHttp.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build:types": "tsc -b ./tsconfig.json",
    "build": "npm run build:types && tsc && vite build"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.9",
    "jsencrypt": "^3.3.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.3.1"
  }
}
